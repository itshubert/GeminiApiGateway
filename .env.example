# Example Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to Git!

# Database Connection Strings (PostgreSQL)
GeminiCatalogDbContext="Host=postgres;Port=5432;Database=geminicatalog;Username=postgres;Password=YOUR_PASSWORD"
GeminiCustomerDbContext="Host=postgres;Port=5432;Database=geminicustomer;Username=postgres;Password=YOUR_PASSWORD"
GeminiInventoryDbContext="Host=postgres;Port=5432;Database=geminiinventory;Username=postgres;Password=YOUR_PASSWORD"
GeminiOrderDbContext="Host=postgres;Port=5432;Database=geminiorder;Username=postgres;Password=YOUR_PASSWORD"
GeminiOrderFulfillmentDbContext="Host=postgres;Port=5432;Database=geminifulfillment;Username=postgres;Password=YOUR_PASSWORD"
GeminiWarehouseDbContext="Host=postgres;Port=5432;Database=geminiwarehouse;Username=postgres;Password=YOUR_PASSWORD"

# Certificate Password
CERTIFICATE_PASSWORD=YourSecurePasswordHere

# Optional: SQS Queue URLs (if you want to include them here)
